{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch BlazorRummiSolve",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/BlazorRummiSolve/bin/Debug/net9.0/BlazorRummiSolve.dll",
            "args": [],
            "cwd": "${workspaceFolder}/BlazorRummiSolve",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "Launch RummiSolve",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/RummiSolve/RummiSolve/bin/Debug/net9.0/RummiSolve.dll",
            "args": [],
            "cwd": "${workspaceFolder}/RummiSolve/RummiSolve",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
} 